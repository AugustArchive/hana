{
  "name": "@august/hana",
  "description": "èŠ± (\"hana\") is a API to proxy different APIs",
  "version": "3.0.2",
  "main": "build/main.js",
  "author": "August <cutie@floofy.dev>",
  "scripts": {
    "clean:node_modules": "rm -rf node_modules/**/node_modules && rm -rf node_modules/@types/**/node_modules && rm -rf node_modules/@augu/**/node_modules",
    "clean:win:tar": "cp node_modules/@augu/collections/build/index.js.* node_modules/@augu/collections/build/index.js && rm node_modules/@augu/collections/build/index.js.*",
    "build": "npm run lint && rm -rf build && tsc",
    "start": "cd build && node main.js",
    "lint": "eslint src --ext .ts --fix",
    "dev": "cd src && ts-node --files --project ../tsconfig.json main.ts"
  },
  "dependencies": {
    "@augu/http": "1.2.1",
    "@augu/lilith": "3.0.2",
    "@augu/orchid": "3.1.1",
    "@augu/utils": "1.5.3",
    "@aws-sdk/client-s3": "3.16.0",
    "fastify": "3.15.1",
    "fastify-cors": "6.0.1",
    "fastify-no-icon": "4.0.0",
    "fastify-rate-limit": "5.5.0",
    "image-size": "1.0.0",
    "js-yaml": "4.1.0",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.19",
    "tslog": "3.2.0"
  },
  "devDependencies": {
    "@augu/eslint-config": "2.0.3",
    "@augu/tsconfig": "1.1.0",
    "@types/js-yaml": "4.0.1",
    "@types/node": "15.3.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "eslint": "7.26.0",
    "husky": "6.0.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4"
  }
}
